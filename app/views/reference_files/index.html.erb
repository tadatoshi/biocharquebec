<h1>Listing reference_files</h1>

<table>
  <tr>
    <th>File</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @reference_files.each do |reference_file| %>
  <tr>
    <td><%= reference_file.file %></td>
    <td><%= link_to 'Show', reference_file %></td>
    <td><%= link_to 'Edit', edit_reference_file_path(reference_file) %></td>
    <td><%= link_to 'Destroy', reference_file, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

